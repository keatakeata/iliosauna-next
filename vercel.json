{
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/next",
      "config": {
        "maxDuration": 300
      }
    }
  ],
  "functions": {
    "src/app/**/*.tsx": {
      "maxDuration": 30
    }
  },
  "github": {
    "autoAlias": false
  },
  "env": {
    "NODE_OPTIONS": "--max-old-space-size=8192"
  },
  "buildCommand": "npm run force-deploy"
}